      </assemblies>
    </compilation>

    <authentication mode="Windows"/>
    <pages controlRenderingCompatibilityVersion="3.5" enableViewStateMac="false" enableEventValidation="false" viewStateEncryptionMode="Never" clientIDMode="AutoID">
